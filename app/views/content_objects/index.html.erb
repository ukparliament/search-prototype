<% content_for(:before_title) do %>
  <%= link_to('UK Parliament', root_path) %> / <%= link_to('Open data', root_path) %><br/>
  <div id="top"></div>
<% end %>

<section id="search" class="content-section white">
  <h2 class="content-heading">Search</h2>
  <%= form_with url: search_url do |f| %>
    <%= f.text_field :query %>
    <%= f.submit 'Search' %>
  <% end %>
</section>

